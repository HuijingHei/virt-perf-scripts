    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: GenerateNetworkBenchmarkReport.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/network/GenerateNetworkBenchmarkReport.py",
            "console": "integratedTerminal",
            "pythonPath": "${command:python.interpreterPath}",
            "args": [
                "--base_csv",
                "network/results/flent_report-base.csv",
                "--test_csv",
                "network/results/flent_report-test.csv",
                "--report_csv",
                "network/results/flent_benchmark_report.csv"
            ]
        },
        {
            "name": "Python: GenerateNetworkTestReport.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/network/GenerateNetworkTestReport.py",
            "console": "integratedTerminal",
            "pythonPath": "${command:python.interpreterPath}",
            "args": [
                "--result_path",
                "network/results"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "stopOnEntry": true,
            "pythonPath": "${command:python.interpreterPath}",
            "program": "${file}",
            "cwd": "${workspaceFolder}",
            "env": {},
            "envFile": "${workspaceFolder}/.env",
        }
    ]
}